{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true,
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true,
    "blockBindings": true,
    "jsx": true
  },
  "rules": {
    "new-cap": 0,
    "id-length": 0,
    "key-spacing": 0,
    "no-multi-spaces": 0,
    "react/jsx-boolean-value": 0,
  },
}


